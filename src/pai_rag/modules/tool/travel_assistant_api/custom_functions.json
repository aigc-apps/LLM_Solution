[
  {
    "type": "function",
    "function": {
      "name": "search_flight_ticket",
      "description": "帮助用户获取机票信息，用户需要输入出发地、目的地",
      "parameters": {
        "type": "object",
        "properties": {
          "arr_airport": {
            "type": "str",
            "description": "到达的机场，如'上海虹桥'"
          },
          "arr_city": {
            "type": "str",
            "description": "目的地城市，如'北京'、'上海'、'南京'"
          },
          "dep_airport": {
            "type": "str",
            "description": "出发的机场，如'大兴机场'"
          },
          "dep_city": {
            "type": "str",
            "description": "出发城市，如'北京'、'上海'、'南京'"
          },
          "flight_date": {
            "type": "str",
            "description": "出发时间，如'2024-03-29'"
          }
        },
        "required": [
          "arr_airport",
          "arr_city",
          "dep_airport",
          "dep_city",
          "flight_date"
        ]
      }
    }
  },
  {
    "type": "function",
    "function": {
      "name": "search_train_ticket",
      "description": "查询火车票或者高铁票信息",
      "parameters": {
        "type": "object",
        "properties": {
          "start_city": {
            "type": "str",
            "description": "出发城市，如'长沙'"
          },
          "end_city": {
            "type": "str",
            "description": "终点城市，如'青岛'"
          },
          "train_date": {
            "type": "str",
            "description": "出发日期，如'2024-03-29'"
          }
        },
        "required": ["start_city", "end_city", "train_date"]
      }
    }
  }
]
